Algoritmo "questao5"

Var
   ladoQuadrado : real

Inicio
      escreval("Calculando a área de um quadrado qualquer")
      escreva("Digite o valor em centímetros do lado do quadrado: ")
      leia(ladoQuadrado)
      ladoQuadrado <- ladoQuadrado^2
      escreva("A área do quadrado é de ", ladoQuadrado, "cm²")


Fimalgoritmo